<!-- Body -->
<div class="container">
  <h3>Detail Contact</h3>

  <% if (detail.name) { %>
  <table class="table table-striped">
    <tr></tr>
    <tr>
      <td style="font-weight: bold;">Name</td>
      <td><%= detail.name %> </td>
    </tr>
    <tr>
      <td style="font-weight: bold;">Email</td>
      <td><%= detail.email %> </td>
    </tr>
    <tr>
      <td style="font-weight: bold;">Mobile</td>
      <td><%= detail.mobile  %></td>
    </tr>
    </tr>
    <% } else{%>
    <h3 colspan="2" class="text-center text-danger">Data Kontak Tidak ada</h3>
    <% }%>

  </table>
  <a href="/contact" class="btn btn-dark">Kembali</a>
</div>